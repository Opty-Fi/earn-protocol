{
    "extends": "solhint:recommended",
    "rules": {
        "func-order": "off",
        "mark-callable-contracts": "off",
        "no-empty-blocks": "off",
        "compiler-version": ["error", "^0.6.10"],
        "private-vars-leading-underscore": "off",
        "reason-string": "off"
    },
    "plugins": ["prettier"]
}
